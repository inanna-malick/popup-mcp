{
  "title": "Feedback Form",
  "elements": [
    {
      "text": "We'd love to hear from you!",
      "id": "intro_text"
    },
    {
      "textbox": "Name",
      "id": "name",
      "placeholder": "Your name"
    },
    {
      "textbox": "Email",
      "id": "email",
      "placeholder": "your@email.com"
    },
    {
      "multiselect": "Rating",
      "id": "rating",
      "options": ["Excellent", "Good", "Fair", "Poor"]
    },
    {
      "textbox": "Comments",
      "id": "comments",
      "placeholder": "Tell us what you think...",
      "rows": 5
    },
    {
      "multiselect": "Topics",
      "id": "topics",
      "options": ["UI/UX", "Performance", "Features", "Documentation", "Support"]
    },
    {
      "checkbox": "Subscribe to newsletter",
      "id": "subscribe",
      "default": false
    }
  ]
}
