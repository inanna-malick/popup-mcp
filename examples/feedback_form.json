{
  "title": "Feedback Form",
  "elements": [
    {
      "text": "We'd love to hear from you!",
      "id": "intro_text"
    },
    {
      "id": "name",
      "placeholder": "Your name",
      "input": "Name"
    },
    {
      "id": "email",
      "placeholder": "your@email.com",
      "input": "Email"
    },
    {
      "id": "rating",
      "options": [
        "Excellent",
        "Good",
        "Fair",
        "Poor"
      ],
      "multi": "Rating"
    },
    {
      "id": "comments",
      "placeholder": "Tell us what you think...",
      "rows": 5,
      "input": "Comments"
    },
    {
      "id": "topics",
      "options": [
        "UI/UX",
        "Performance",
        "Features",
        "Documentation",
        "Support"
      ],
      "multi": "Topics"
    },
    {
      "id": "subscribe",
      "default": false,
      "check": "Subscribe to newsletter"
    }
  ]
}