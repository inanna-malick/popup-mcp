{
  "title": "User Profile",
  "elements": [
    {
      "textbox": "Username",
      "id": "username",
      "placeholder": "Enter username"
    },
    {
      "textbox": "Full Name",
      "id": "fullname",
      "placeholder": "John Doe"
    },
    {
      "textbox": "Email",
      "id": "email",
      "placeholder": "john@example.com"
    },
    {
      "textbox": "Bio",
      "id": "bio",
      "placeholder": "Tell us about yourself...",
      "rows": 4
    },
    {
      "multiselect": "Country",
      "id": "country",
      "options": ["United States", "Canada", "United Kingdom", "Australia", "Other"]
    },
    {
      "multiselect": "Interests",
      "id": "interests",
      "options": ["Technology", "Sports", "Music", "Art", "Gaming", "Travel", "Food"]
    },
    {
      "checkbox": "Profile visible to others",
      "id": "visible",
      "default": true
    },
    {
      "checkbox": "Receive email notifications",
      "id": "notifications",
      "default": true
    }
  ]
}
