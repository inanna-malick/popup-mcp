{
  "title": "User Profile",
  "elements": [
    {
      "id": "username",
      "placeholder": "Enter username",
      "input": "Username"
    },
    {
      "id": "fullname",
      "placeholder": "John Doe",
      "input": "Full Name"
    },
    {
      "id": "email",
      "placeholder": "john@example.com",
      "input": "Email"
    },
    {
      "id": "bio",
      "placeholder": "Tell us about yourself...",
      "rows": 4,
      "input": "Bio"
    },
    {
      "id": "country",
      "options": [
        "United States",
        "Canada",
        "United Kingdom",
        "Australia",
        "Other"
      ],
      "multi": "Country"
    },
    {
      "id": "interests",
      "options": [
        "Technology",
        "Sports",
        "Music",
        "Art",
        "Gaming",
        "Travel",
        "Food"
      ],
      "multi": "Interests"
    },
    {
      "id": "visible",
      "default": true,
      "check": "Profile visible to others"
    },
    {
      "id": "notifications",
      "default": true,
      "check": "Receive email notifications"
    }
  ]
}